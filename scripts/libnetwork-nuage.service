[Unit]
Description=Nuage libnetwork plugin for docker
Wants=docker.service
After=network.target docker.service

[Service]
ExecStart=/usr/bin/libnetwork-nuage -config /etc/default/libnetwork-nuage.yaml
Type=simple
PIDFile=/var/run/libnetwork-nuage/libnetwork-nuage.pid
Restart=always

[Install]
WantedBy=multi-user.target
